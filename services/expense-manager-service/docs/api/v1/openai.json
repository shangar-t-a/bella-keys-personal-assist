                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "MonthYearRequest": {
                "properties": {
                    "month": {
                        "type": "string",
                        "enum": [
                            "January",
                            "February",
                            "March",
                            "April",
                            "May",
                            "June",
                            "July",
                            "August",
                            "September",
                            "October",
                            "November",
                            "December"
                        ],
                        "title": "Month",
                        "description": "Name of the month"
                    },
                    "year": {
                        "type": "integer",
                        "maximum": 2100,
                        "minimum": 2000,
                        "title": "Year",
                        "description": "Year between 2000 and 2100",
                        "examples": [
                            2024,
                            2025
                        ]
                    }
                },
                "additionalProperties": false,
                "type": "object",
                "required": [
                    "month",
                    "year"
                ],
                "title": "MonthYearRequest",
                "description": "Schema for month-year creation requests."
            },
            "MonthYearResponse": {
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Id",
                        "description": "Unique identifier for the month-year combination"
                    },
                    "month": {
                        "type": "string",
                        "enum": [
                            "January",
                            "February",
                            "March",
                            "April",
                            "May",
                            "June",
                            "July",
                            "August",
                            "September",
                            "October",
                            "November",
                            "December"
                        ],
                        "title": "Month",
                        "description": "Name of the month"
                    },
                    "year": {
                        "type": "integer",
                        "title": "Year",
                        "description": "Year value"
                    }
                },
                "additionalProperties": false,
                "type": "object",
                "required": [
